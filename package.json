{
	"name": "rfdprint",
	"homepage": "https://rfdprint.github.io/rfdprint.com",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@material-ui/core": "^4.5.1",
		"cuid": "^2.1.6",
		"firebase": "^7.2.1",
		"gh-pages": "^2.1.1",
		"normalize.css": "^8.0.1",
		"react": "^16.11.0",
		"react-app": "^1.1.2",
		"react-dom": "^16.11.0",
		"react-redux": "^7.1.1",
		"react-router-dom": "^5.1.2",
		"react-scripts": "3.2.0",
		"redux": "^4.0.4",
		"sanitize-html": "^1.20.1",
		"scripts": "^0.1.0"
	},
	"scripts": {
		"build:css": "postcss src/styles/main-src.css -o src/index.css",
		"watch:css": "postcss src/styles/main-src.css -o src/index.css -w",
		"build:vender-css": "postcss src/styles/vender-src.css -o src/vender.css",
		"watch:vender-css": "postcss src/styles/vender-src.css -o src/vender.css -w",
		"start": "npm-run-all -p watch:css watch:vender-css start-js",
		"start-js": "react-scripts start",
		"build-js": "react-scripts build",
		"build": "npm-run-all build:css build:vender-css build-js",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"autoprefixer": "^9.7.0",
		"eslint-loader": "^3.0.2",
		"npm-run-all": "^4.1.5",
		"postcss-cli": "^6.1.3",
		"postcss-color-function": "^4.1.0",
		"postcss-hexrgba": "^2.0.0",
		"postcss-import": "^12.0.1",
		"postcss-mixins": "^6.2.2",
		"postcss-nested": "^4.1.2",
		"postcss-simple-vars": "^5.0.2",
		"redux-thunk": "^2.3.0"
	}
}
